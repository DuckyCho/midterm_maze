package maze.maze;

import maze.utils.*;

public class MazeFloor {
	public int[][] floor;
	
	public MazeFloor(){
		floor = new int[10][10];
		for(int i = 0 ; i < Defines.basicMazeSize ; i++){
			for(int j = 0 ; j< Defines.basicMazeSize ; j++){
				floor[i][j] = 0;
			}
		}
	}
	
	
	
	/*정방형 미로 바닥을 만든다.*/
	public MazeFloor(int width){
		floor = new int[width][width];
		
		for(int i = 0 ; i < width ; i++){
			for(int j = 0 ; j< width ; j++){
				floor[i][j] = 0;
			}
		}
	}
}
